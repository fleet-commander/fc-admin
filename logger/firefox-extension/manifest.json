{

  "manifest_version": 2,
  "name": "Fleet Commander logger",
  "version": "1.0.4",

  "description": "Enables bookmark monitoring for Fleet Commander logger",

  "permissions": [
    "bookmarks",
    "nativeMessaging"
  ],
  
  "browser_specific_settings": {
    "gecko": {
      "id": "{c73e87a7-b5a1-4b6f-b10b-0bd70241a64d}",
      "strict_min_version": "60.0"
    }
  },

  "icons": {
    "48": "icons/fc-48.png",
    "96": "icons/fc-96.png"
  },

  "background": {
    "scripts": ["bookmarklogger.js"]
  }
}
